{
    "env": {
        "cudaIncludePath": "${workspaceFolder}/include/CUDA-10.2",
        "libcxxInlucdePath": "${cudaIncludePath}/cuda/std/detail/libcxx/include",
        "glibcInlucdePath": "${workspaceFolder}/include/glibc-2.31"
    },
    "configurations": [
        {
            "name": "CUDA",
            "defines": [
                "__CUDACC__",
                "__GNUC__"
            ],
            "includePath": [
                "${workspaceFolder}/include/ginclude-9.2.0",
                "${glibcInlucdePath}/include",
                "${glibcInlucdePath}/generic",
                "${glibcInlucdePath}/sysdeps/x86",
                "${glibcInlucdePath}/sysdeps/x86_64",
                "${glibcInlucdePath}",
                "${libcxxInlucdePath}",
                "${cudaIncludePath}"
            ],
            "forcedInclude": [
                "${libcxxInlucdePath}/cstdlib",
                "${cudaIncludePath}/cuda_runtime_api.h",
                "${cudaIncludePath}/device_launch_parameters.h",
                "${cudaIncludePath}/device_functions.h"
            ]
        }
    ],
    "version": 4
}