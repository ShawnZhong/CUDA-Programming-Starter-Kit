{
    "env": {
        "cudaIncludePath": "${workspaceFolder}/include/CUDA-10.2",
        "libcxxInlucdePath": "${cudaIncludePath}/cuda/std/detail/libcxx/include"
    },
    "configurations": [{
        "name": "CUDA",
        "defines": [
            "__CUDACC__"
        ],
        "includePath": [
            "${workspaceFolder}/include/ginclude-9.2.0",
            "${workspaceFolder}/include/glibc-2.31/include",
            "${workspaceFolder}/include/glibc-2.31/sysdeps/generic",
            "${workspaceFolder}/include/glibc-2.31/sysdeps/x86",
            "${workspaceFolder}/include/glibc-2.31/sysdeps/x86_64",
            "${workspaceFolder}/include/glibc-2.31",
            "${libcxxInlucdePath}",
            "${cudaIncludePath}"
        ],
        "forcedInclude": [
            "${libcxxInlucdePath}/cstdlib",
            "${cudaIncludePath}/cuda_runtime_api.h",
            "${cudaIncludePath}/device_launch_parameters.h",
            "${cudaIncludePath}/device_functions.h"
        ]
    }],
    "version": 4
}